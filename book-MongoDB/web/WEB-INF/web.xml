<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>
    
    <servlet>
        <servlet-name>EnteryServlet</servlet-name>
        <servlet-class>web1.EnteryServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EnteryServlet</servlet-name>
        <url-pattern>/enteryServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DelServlet</servlet-name>
        <servlet-class>web1.DelServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DelServlet</servlet-name>
        <url-pattern>/delServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ChangeBookServlet</servlet-name>
        <servlet-class>web1.ChangeBookServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ChangeBookServlet</servlet-name>
        <url-pattern>/changeBookServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>BookServlet</servlet-name>
        <servlet-class>web.BookServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BookServlet</servlet-name>
        <url-pattern>/bookServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UserServlet</servlet-name>
        <servlet-class>web.UserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserServlet</servlet-name>
        <url-pattern>/userServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ReaderServlet</servlet-name>
        <servlet-class>web.ReaderServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ReaderServlet</servlet-name>
        <url-pattern>/readerServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>BorrowServlet</servlet-name>
        <servlet-class>web.BorrowServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BorrowServlet</servlet-name>
        <url-pattern>/borrowServlet</url-pattern>
    </servlet-mapping>
</web-app>